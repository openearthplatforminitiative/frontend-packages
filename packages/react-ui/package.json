{
  "name": "@openepi/react-ui",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "build": "tsc",
    "prepare": "panda codegen"
  },
  "dependencies": {
    "@ark-ui/react": "^4.4.4",
    "@openepi/icons": "*",
    "@zag-js/anatomy": "^0.77.1",
    "lodash": "^4.17.21"
  },
  "devDependencies": {
    "@pandacss/dev": "^0.47.1",
    "@types/lodash": "^4.17.13",
    "@types/react": "^18.3.12",
    "postcss": "^8.4.47"
  },
  "exports": {
    "./preset": {
      "import": "./dist/theme/index.js",
      "require": "./dist/theme/index.js"
    },
    "./styled-system": {
      "types": "./dist/styled-system/**/*.d.ts",
      "import": "./dist/styled-system/**/*.js",
      "require": "./dist/styled-system/**/*.js"
    },
    "./styled-system/styles": {
      "import": "./dist/styled-system/styles/*.js",
      "require": "./dist/styled-system/styles/*.js"
    },
    "./*": {
      "types": "./dist/components/*.d.ts",
      "import": "./dist/components/*.js",
      "require": "./dist/components/*.js"
    }
  }
}